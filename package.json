{
  "name": "brick",
  "version": "1.0.0",
  "main": "lib/index",
  "sideEffects": false,
  "repository": "https://github.com/sweetlikepete/brick",
  "author": "Peter Bodechtel <sweetlikepete@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf lib && npm run build:script",
    "build:script": "npm run build:script:module && npm run build:script:common",
    "build:script:common": "babel src --out-dir lib --extensions '.js,.ts'",
    "build:script:module": "OUTPUT_MODULES=1 npm run build:script:common && renamer --find js --replace mjs lib/**",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@sweetlikepete/eslint-config": "/Users/sweetlikepete/code/brick-test/sweetlikepete-eslint-config-1.0.13.tgz",
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "@typescript-eslint/parser": "^1.3.0",
    "eslint": "^5.13.0",
    "eslint-plugin-array-func": "^3.1.2",
    "eslint-plugin-compat": "^2.7.0",
    "eslint-plugin-eslint-comments": "^3.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-optimize-regex": "^1.1.6",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^7.1.0",
    "typescript": "^3.3.3"
  },
  "devDependencies": {
    "renamer": "^1.1.1"
  }
}
