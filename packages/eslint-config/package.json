{
  "name": "@sweetlikepete/eslint-config",
  "version": "1.0.21",
  "description": "Shared config for eslint",
  "main": "lib/configs",
  "scripts": {
    "build": "rm -rf lib && babel src --config-file --out-dir lib",
    "lint": "eslint src test --ext .jsx,.js,.tsx,.ts",
    "xlint": "eslint src test --ext .jsx,.js,.tsx,.ts && eslint-find-rules lib/index.js --unused",
    "prelint": "npm run build",
    "prepublishOnly": "npm run test",
    "pretest": "npm run lint",
    "test": "babel-tape-runner ./test/*.js"
  },
  "browserslist": "node 8",
  "engines": {
    "node": ">= 8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sweetlikepete/brick"
  },
  "author": "Peter Bodechtel (https://www.sweetlikepete.com)",
  "contributors": [
    {
      "name": "Peter Bodechtel",
      "url": "https://www.sweetlikepete.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sweetlikepete/brick/issues"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config",
    "sweetlikepete",
    "@sweetlikepete"
  ],
  "homepage": "https://github.com/sweetlikepete/brick/tree/master/packages/eslint-config#readme",
  "devDependencies": {
    "@sweetlikepete/brick": "^1.0.5",
    "babel-tape-runner": "^3.0.0",
    "tape": "^4.9.2"
  },
  "dependencies": {
    "confusing-browser-globals": "^1.0.5"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "@typescript-eslint/parser": "^1.3.0",
    "eslint": "^5.13.0",
    "eslint-plugin-array-func": "^3.1.2",
    "eslint-plugin-compat": "^2.7.0",
    "eslint-plugin-eslint-comments": "^3.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-optimize-regex": "^1.1.6",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^7.1.0",
    "typescript": "^3.3.3"
  }
}
