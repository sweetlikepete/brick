{
  "name": "@sweetlikepete/scotch",
  "version": "1.0.3",
  "description": "Node.js SSR application framework",
  "main": "lib",
  "types": "lib",
  "scripts": {
    "build": "rm -rf lib && npm run build:script && npm run build:static && npm run build:types",
    "build:script": "npm run build:script:module && npm run build:script:common",
    "build:script:common": "babel src --ignore src/cli --out-dir lib --extensions '.js,.jsx,.ts,.tsx'",
    "build:script:module": "BUILD_MJS=1 npm run build:script:common && renamer --find js --replace mjs \"lib/**\"",
    "build:static": "cp -R src/static lib/static",
    "build:types": "tsc --emitDeclarationOnly",
    "lint": "eslint src test --ext .js,.jsx,.ts,.tsx",
    "prelint": "npm run build",
    "prepublishOnly": "npm run test",
    "pretest": "npm run lint",
    "test": "babel-tape-runner ./test/*.js"
  },
  "browserslist": "node 8",
  "engines": {
    "node": ">= 8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sweetlikepete/brick.git"
  },
  "author": "Peter Bodechtel (https://www.sweetlikepete.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sweetlikepete/brick/issues"
  },
  "homepage": "https://github.com/sweetlikepete/brick/tree/master/packages/scotch#readme",
  "devDependencies": {
    "@sweetlikepete/babel": "^1.0.5",
    "@sweetlikepete/eslint": "^1.0.18",
    "@types/common-tags": "^1.8.0",
    "@types/compression": "^0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.1",
    "@types/express-graphql": "^0.8.0",
    "@types/express-jwt": "^0.0.42",
    "@types/graphql": "^14.2.0",
    "@types/helmet": "^0.0.43",
    "@types/js-cookie": "^2.2.2",
    "@types/path-to-regexp": "^1.7.0",
    "@types/react": "16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-frontload": "^1.0.0",
    "@types/react-loadable": "^5.5.1",
    "@types/react-redux": "7.0.9",
    "@types/react-router-dom": "^4.3.3",
    "@types/react-test-renderer": "^16.8.1",
    "@types/webpack-dev-middleware": "^2.0.2",
    "@types/webpack-env": "^1.13.9",
    "@types/webpack-hot-middleware": "^2.16.5",
    "babel-tape-runner": "^3.0.0",
    "renamer": "^1.1.1",
    "tape": "^4.10.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@google-cloud/logging-winston": "^0.11.1",
    "@types/react-helmet": "^5.0.8",
    "apollo-client": "^2.6.0",
    "body-parser": "^1.19.0",
    "common-tags": "^1.8.0",
    "compression": "^1.7.4",
    "connected-react-router": "^6.4.0",
    "cookie-parser": "^1.4.4",
    "deepmerge": "^3.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.8.0",
    "express-jwt": "^5.3.1",
    "express-slash": "^2.0.1",
    "graphql": "^14.3.1",
    "helmet": "^3.18.0",
    "history": "^4.9.0",
    "js-cookie": "^2.2.0",
    "prop-types": "^15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-frontload": "^1.0.7",
    "react-helmet": "^5.2.1",
    "react-helmet-async": "^1.0.2",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-ssr-prepass": "^1.0.5",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "type-graphql": "^0.17.4",
    "webpack": "^4.32.2",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "winston": "^3.2.1"
  }
}
